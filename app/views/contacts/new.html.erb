<!doctype html>
<!--[if IE 9]><html class="lt-ie10" lang="en" > <![endif]-->
<!--[if IE 10]><html class="ie10" lang="en" > <![endif]-->
<html class="no-js" lang="en">

<head>

  <meta charset="UTF-8">

  <title>Steve Buckley | Portfolio</title>

  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
  <meta name="description" content="">
  <meta name="author" content="">


  <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
  <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
  <![endif]-->

  <!-- Modernizr Scripts -->
  <script src="js/modernizr-2.6.2.min.js"></script>

</head>

<body>




  <!-- TOP BAR -->
  <header class="masthead">

    <div class="row">

      <nav class="top-bar" data-topbar>

        <ul class="title-area">

          <!-- Title Area -->
          <li class="name">
            <h1>Portfolio</h1>
          </li>

          <!-- Remove the class "menu-icon" to get rid of menu icon. Take out "Menu" to just have icon alone -->
          <li class="toggle-topbar menu-icon"><a href="#"><span>Menu</span></a></li>

        </ul><!-- /.title-area -->

        <section class="top-bar-section">

          <!-- Right Nav Section -->
          <ul class="right">

            <li class="divider"></li>
            <li><%= link_to "Home", root_path %></a></li>
            <li class="divider"></li>

          </ul><!-- /.right -->

        </section><!-- /.top-bar-section -->

      </nav><!-- /.top-bar -->

    </div><!-- /.row -->

  </header><!-- /.masthead -->
  <!-- TOP BAR -->




  <!-- CONTACT -->
  <section class="contact-wrapper">
    <div class="row">
      <div class="medium-8 small-centered columns">
        <h2>Contact me</h2>
      </div><!-- /.medium-12 .small-centered .columns -->
    </div><!-- /.row -->


    <!-- Start: Contact form-->
      <div class="row">

        <div class="medium-6 columns">

          <%= form_for @contact do |f| %>
            <%= f.label :name %>
            <%= f.text_field :name %>

            <%= f.label :email %>
            <%= f.email_field :email %>

            <%= f.label :message %>
            <%= f.text_area :message %>

            <%= f.submit 'Submit' %>
          <% end %>

        </div><!-- /.medium-6 .columns -->
        <div class="medium-6 columns">

        </div><!-- /.medium-6 .columns -->

      </div><!-- /.row -->
    <!-- End: Contact form-->





  </section><!-- /.contact-wrapper -->
  <!-- CONTACT -->




  <!-- PRE FOOTER -->
  <section class="pre-footer">

    <div class="row">

      <div class="medium-4 columns">
        <div class="col-pre-footer">
          <h5>Address</h5>
          <ul class="add-links">
            <li><strong><i class="fi-marker"></i> Steve Buckley</strong> <br>5706 SW Admiral Way, Seattle, WA  98116</li>
            <li><i class="fi-telephone"></i> (425) 999-9634</li>
            <li><i class="fi-mail"></i> buckleys78@gmail.com <em>or</em> steve-buckley@gerrit-tech.com</li>
          </ul><!-- /.add-links -->
        </div><!-- /.col-pre-footer -->
      </div><!-- /.medium-4 .columns -->

    </div><!-- /.row -->

  </section><!-- /.pre-footer -->
  <!-- PRE FOOTER -->

  <!-- FOOTER -->
  <!-- FOOTER -->

  <!-- Load JS here for greater good -->
  <script src="js/jquery-1.10.2.min.js"></script>
  <script src="js/foundation.min.js"></script>
  <script src="js/smoothscroll.js"></script>
  <script src="js/main.js"></script>

  <!--
    You need to include this script on any page that has a Google Map.
    When using Google Maps on your own site you "MUST" signup for your own API key () at:
    https://developers.google.com/maps/documentation/javascript/tutorial#api_key

    After your sign up replace the key in the URL below or paste in the new script tag that Google provides.
  -->
  <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyASm3CwaK9qtcZEWYa-iQwHaGi3gcosAJc&amp;sensor=false"></script>
  <!-- Google Maps Script (to edit your styles, latitude and longitude for center the map) -->
  <script src="js/maps.js"></script>




</body>

</html>
